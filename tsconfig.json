{
  "compilerOptions": {
    "outDir": "./dist",
    "incremental": true,
    "target": "ES2020",
    "module": "ES6",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "skipLibCheck": true
  }
}
